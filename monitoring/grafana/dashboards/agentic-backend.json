{
  "dashboard": {
    "title": "Agentic Backend - Production Monitoring",
    "uid": "agentic-backend-main",
    "version": 1,
    "timezone": "browser",
    "schemaVersion": 38,
    "editable": true,
    "fiscalYearStartMonth": 0,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 0},
        "type": "stat",
        "title": "HTTP Requests (Total)",
        "targets": [
          {
            "expr": "sum(http_requests_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "thresholds"},
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 1000, "color": "yellow"},
                {"value": 10000, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 2,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 0},
        "type": "stat",
        "title": "Active Sessions",
        "targets": [
          {
            "expr": "active_sessions",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 3,
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 6},
        "type": "graph",
        "title": "HTTP Request Rate (req/sec)",
        "targets": [
          {
            "expr": "rate(http_requests_total[5m])",
            "refId": "A",
            "legendFormat": "{{method}} {{endpoint}} ({{status}})"
          }
        ],
        "yaxes": [
          {"label": "Requests/sec", "format": "short"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 4,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 14},
        "type": "graph",
        "title": "HTTP Request Latency (p50, p90, p99)",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(http_request_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "p50"
          },
          {
            "expr": "histogram_quantile(0.90, rate(http_request_duration_seconds_bucket[5m]))",
            "refId": "B",
            "legendFormat": "p90"
          },
          {
            "expr": "histogram_quantile(0.99, rate(http_request_duration_seconds_bucket[5m]))",
            "refId": "C",
            "legendFormat": "p99"
          }
        ],
        "yaxes": [
          {"label": "Latency (seconds)", "format": "s"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 5,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 14},
        "type": "graph",
        "title": "Agent Query Rate by Type",
        "targets": [
          {
            "expr": "rate(agent_queries_total{status=\"success\"}[5m])",
            "refId": "A",
            "legendFormat": "{{agent_type}}"
          }
        ],
        "yaxes": [
          {"label": "Queries/sec", "format": "short"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 6,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 22},
        "type": "graph",
        "title": "Agent Query Duration by Type",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(agent_query_duration_seconds_bucket[5m]))",
            "refId": "A",
            "legendFormat": "{{agent_type}} (p95)"
          }
        ],
        "yaxes": [
          {"label": "Duration (seconds)", "format": "s"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 7,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 22},
        "type": "graph",
        "title": "API Cost by Model (USD)",
        "targets": [
          {
            "expr": "rate(api_cost_dollars_total[1h])",
            "refId": "A",
            "legendFormat": "{{model}}"
          }
        ],
        "yaxes": [
          {"label": "Cost/hour (USD)", "format": "currencyUSD"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 8,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 30},
        "type": "graph",
        "title": "Token Usage by Model",
        "targets": [
          {
            "expr": "rate(api_tokens_total{type=\"input\"}[5m])",
            "refId": "A",
            "legendFormat": "{{model}} (input)"
          },
          {
            "expr": "rate(api_tokens_total{type=\"output\"}[5m])",
            "refId": "B",
            "legendFormat": "{{model}} (output)"
          }
        ],
        "yaxes": [
          {"label": "Tokens/sec", "format": "short"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 9,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 30},
        "type": "graph",
        "title": "Cache Hit Rate",
        "targets": [
          {
            "expr": "rate(cache_operations_total{result=\"hit\"}[5m]) / (rate(cache_operations_total{result=\"hit\"}[5m]) + rate(cache_operations_total{result=\"miss\"}[5m]))",
            "refId": "A",
            "legendFormat": "{{cache_type}}"
          }
        ],
        "yaxes": [
          {"label": "Hit Rate", "format": "percentunit"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 10,
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 38},
        "type": "stat",
        "title": "Total API Cost (USD)",
        "targets": [
          {
            "expr": "sum(api_cost_dollars_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 11,
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 38},
        "type": "stat",
        "title": "Total Tokens Used",
        "targets": [
          {
            "expr": "sum(api_tokens_total)",
            "refId": "A"
          }
        ],
        "options": {
          "graphMode": "area",
          "colorMode": "value",
          "textMode": "value_and_name"
        },
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "color": {"mode": "palette-classic"}
          }
        }
      },
      {
        "id": 12,
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 44},
        "type": "graph",
        "title": "Authentication Success/Failure Rate",
        "targets": [
          {
            "expr": "rate(auth_attempts_total{result=\"success\"}[5m])",
            "refId": "A",
            "legendFormat": "Success"
          },
          {
            "expr": "rate(auth_attempts_total{result=\"failure\"}[5m])",
            "refId": "B",
            "legendFormat": "Failure"
          }
        ],
        "yaxes": [
          {"label": "Attempts/sec", "format": "short"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      },
      {
        "id": 13,
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 44},
        "type": "graph",
        "title": "Permission Checks (Allowed/Denied)",
        "targets": [
          {
            "expr": "rate(permission_checks_total{result=\"allowed\"}[5m])",
            "refId": "A",
            "legendFormat": "Allowed"
          },
          {
            "expr": "rate(permission_checks_total{result=\"denied\"}[5m])",
            "refId": "B",
            "legendFormat": "Denied"
          }
        ],
        "yaxes": [
          {"label": "Checks/sec", "format": "short"},
          {"show": false}
        ],
        "xaxis": {"mode": "time", "show": true},
        "legend": {"show": true, "values": true, "current": true}
      }
    ],
    "refresh": "30s",
    "time": {
      "from": "now-6h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h", "2h", "1d"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d", "30d"]
    },
    "tags": ["agentic", "backend", "production"],
    "templating": {
      "list": []
    }
  },
  "overwrite": true
}
