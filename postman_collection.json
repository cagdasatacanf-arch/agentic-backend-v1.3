{
  "info": {
    "name": "Agentic Backend API",
    "description": "Complete API collection for testing the Agentic Backend",
    "schema": "https://schema.getpostman.com/json/collection/v2.1.0/collection.json"
  },
  "auth": {
    "type": "apikey",
    "apikey": [
      {
        "key": "key",
        "value": "X-API-Key",
        "type": "string"
      },
      {
        "key": "value",
        "value": "{{API_KEY}}",
        "type": "string"
      },
      {
        "key": "in",
        "value": "header",
        "type": "string"
      }
    ]
  },
  "item": [
    {
      "name": "Health & Metadata",
      "item": [
        {
          "name": "Health Check",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/v1/health",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "health"]
            }
          }
        },
        {
          "name": "System Metadata",
          "request": {
            "auth": {
              "type": "noauth"
            },
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/v1/metadata",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "metadata"]
            }
          }
        },
        {
          "name": "Agent Health",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/health/agent",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "health", "agent"]
            }
          }
        }
      ]
    },
    {
      "name": "Queries",
      "item": [
        {
          "name": "Simple Query (Calculator)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"What is 25 * 47?\",\n  \"use_rag\": false\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query"]
            }
          }
        },
        {
          "name": "Web Search Query",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"What are the latest AI developments in 2024?\",\n  \"use_rag\": false\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query"]
            }
          }
        },
        {
          "name": "Query with Session",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"My name is Alice\",\n  \"session_id\": \"{{SESSION_ID}}\",\n  \"use_rag\": false\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query"]
            }
          }
        },
        {
          "name": "Follow-up Query (Tests Memory)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"What is my name?\",\n  \"session_id\": \"{{SESSION_ID}}\",\n  \"use_rag\": false\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query"]
            }
          }
        },
        {
          "name": "Streaming Query",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"Explain how AI agents work\",\n  \"use_rag\": false\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query/stream",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query", "stream"]
            }
          }
        }
      ]
    },
    {
      "name": "Documents & RAG",
      "item": [
        {
          "name": "Upload Document",
          "request": {
            "method": "POST",
            "header": [],
            "body": {
              "mode": "formdata",
              "formdata": [
                {
                  "key": "file",
                  "type": "file",
                  "src": ""
                }
              ]
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/docs/upload",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "docs", "upload"]
            }
          }
        },
        {
          "name": "Query with RAG",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"What information is in my documents?\",\n  \"use_rag\": true\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query"]
            }
          }
        },
        {
          "name": "RAG with Custom Parameters",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"Tell me about the topic in my documents\",\n  \"use_rag\": true,\n  \"top_k\": 10,\n  \"score_threshold\": 0.75\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query"]
            }
          }
        }
      ]
    },
    {
      "name": "Session Management",
      "item": [
        {
          "name": "Create Session",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"session_id\": \"{{SESSION_ID}}\",\n  \"metadata\": {\n    \"user_id\": \"user-123\",\n    \"context\": \"testing\"\n  }\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/sessions",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "sessions"]
            }
          }
        },
        {
          "name": "List All Sessions",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/sessions",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "sessions"]
            }
          }
        },
        {
          "name": "Get Session Details",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/sessions/{{SESSION_ID}}",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "sessions", "{{SESSION_ID}}"]
            }
          }
        },
        {
          "name": "Get Session History",
          "request": {
            "method": "GET",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/sessions/{{SESSION_ID}}/history",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "sessions", "{{SESSION_ID}}", "history"]
            }
          }
        },
        {
          "name": "Delete Session",
          "request": {
            "method": "DELETE",
            "header": [],
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/sessions/{{SESSION_ID}}",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "sessions", "{{SESSION_ID}}"]
            }
          }
        }
      ]
    },
    {
      "name": "Error Testing",
      "item": [
        {
          "name": "Invalid API Key (403)",
          "request": {
            "auth": {
              "type": "apikey",
              "apikey": [
                {
                  "key": "key",
                  "value": "X-API-Key",
                  "type": "string"
                },
                {
                  "key": "value",
                  "value": "invalid-key",
                  "type": "string"
                },
                {
                  "key": "in",
                  "value": "header",
                  "type": "string"
                }
              ]
            },
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"question\": \"test\",\n  \"use_rag\": false\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query"]
            }
          }
        },
        {
          "name": "Malformed JSON (422)",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "invalid json content"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/v1/langgraph/query",
              "host": ["{{BASE_URL}}"],
              "path": ["api", "v1", "langgraph", "query"]
            }
          }
        }
      ]
    }
  ],
  "variable": [
    {
      "key": "BASE_URL",
      "value": "http://localhost:8000",
      "type": "string"
    },
    {
      "key": "API_KEY",
      "value": "your-api-key-here",
      "type": "string"
    },
    {
      "key": "SESSION_ID",
      "value": "test-session-123",
      "type": "string"
    }
  ]
}
